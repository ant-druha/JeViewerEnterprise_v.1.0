  </path>
  
  <path id="jeviewerenterprise-war.module.classpath">
    <path refid="${module.jdk.classpath.jeviewerenterprise-war}"/>
    <pathelement location="${jeviewerenterprise-war.output.dir}"/>
    <pathelement location="${basedir}/../JeViewerEnterprise/JeViewerEnterprise-war/build/web/WEB-INF/lib/JeViewerEnterpriseRemoteMod.jar"/>
    <pathelement location="${basedir}/../JeViewerEnterprise/JeViewerEnterprise-ejb/dist/JeViewerEnterprise-ejb.jar"/>
    <path refid="library.glassfish_3.1.2.2.classpath"/>
  </path>
  
  <path id="jeviewerenterprise-war.runtime.module.classpath">
    <pathelement location="${jeviewerenterprise-war.testoutput.dir}"/>
