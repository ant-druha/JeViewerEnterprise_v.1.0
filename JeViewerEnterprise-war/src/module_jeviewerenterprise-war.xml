  </path>
  
  <path id="jeviewerenterprise-war.module.production.classpath">
    <path refid="${module.jdk.classpath.jeviewerenterprise-war}"/>
    <pathelement location="${basedir}/../JeViewerEnterprise/JeViewerEnterprise-war/build/web/WEB-INF/lib/JeViewerEnterpriseRemoteMod.jar"/>
