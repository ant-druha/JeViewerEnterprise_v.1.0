  </path>
  
  
  <target name="compile.module.jeviewerenterprise-war" depends="compile.module.jeviewerenterprise-war.production,compile.module.jeviewerenterprise-war.tests" description="Compile module JeViewerEnterprise-war"/>
  
  <target name="compile.module.jeviewerenterprise-war.production" depends="register.custom.compilers" description="Compile module JeViewerEnterprise-war; production classes">
