  <target name="compile.module.jeviewerenterprise-war.tests" depends="register.custom.compilers,compile.module.jeviewerenterprise-war.production" description="compile module JeViewerEnterprise-war; test classes" unless="skip.tests"/>
  
  <target name="clean.module.jeviewerenterprise-war" description="cleanup module">
    <delete dir="${jeviewerenterprise-war.output.dir}"/>
    <delete dir="${jeviewerenterprise-war.testoutput.dir}"/>
  </target>
</project>