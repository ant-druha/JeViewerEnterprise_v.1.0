      <bootclasspath refid="jeviewerenterprise-war.module.bootclasspath"/>
      <classpath refid="jeviewerenterprise-war.module.production.classpath"/>
      <src refid="jeviewerenterprise-war.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.jeviewerenterprise-war"/>
    </javac2>
    
    <copy todir="${jeviewerenterprise-war.output.dir}">
      <fileset dir="${module.jeviewerenterprise-war.basedir}/java">
