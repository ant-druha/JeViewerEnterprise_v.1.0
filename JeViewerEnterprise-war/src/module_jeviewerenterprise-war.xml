  <patternset id="excluded.from.compilation.jeviewerenterprise-war">
    <patternset refid="excluded.from.module.jeviewerenterprise-war"/>
  </patternset>
  
  <path id="jeviewerenterprise-war.module.sourcepath">
    <dirset dir="${module.jeviewerenterprise-war.basedir}/../../JeViewerEnterprise-war">
