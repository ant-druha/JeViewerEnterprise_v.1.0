  <property name="compiler.args.jeviewerenterprise-war" value="${compiler.args}"/>
  
  <property name="jeviewerenterprise-war.output.dir" value="${module.jeviewerenterprise-war.basedir}/../../../JeViewerDBServer/out/production/JeViewerEnterprise-war"/>
  <property name="jeviewerenterprise-war.testoutput.dir" value="${module.jeviewerenterprise-war.basedir}/../../../JeViewerDBServer/out/test/JeViewerEnterprise-war"/>
  
  <path id="jeviewerenterprise-war.module.bootclasspath">
